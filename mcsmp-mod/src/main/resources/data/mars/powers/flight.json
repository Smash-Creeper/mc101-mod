{
  "type":"origins:multiple",
  "flying": {
    "type": "origins:elytra_flight",
    "render_elytra": false,
    "condition": {
      "type": "origins:and",
      "conditions": [
        {
          "type": "origins:resource",
          "resource": "mars:flight_flying_bar",
          "comparison": ">=",
          "compare_to": 1
        },
        {
          "type": "origins:resource",
          "resource": "mars:flight_bar_on_off",
          "comparison": "<=",
          "compare_to": 0
        }
      ]
    }
  },
  "flying_bar": {
    "type": "origins:resource",
    "min": 0,
    "max": 14,
    "start_value": 0,
    "hud_render": {
      "should_render": true,
      "bar_index": 3,
      "sprite_location": "origins:textures/gui/community/spiderkolo/resource_bar_points_01.png",
      "condition": {
        "type": "origins:resource",
        "resource": "mars:flight_flying_bar",
        "comparison": "<",
        "compare_to": 14
      }
    },
    "hidden": true
  },
  "bar_on_off": {
    "type": "origins:resource",
    "min": 0,
    "max": 3,
    "start_value": 0,
    "hud_render": {
      "should_render": false,
      "bar_index": 5,
      "sprite_location": "origins:textures/gui/community/spiderkolo/resource_bar.png",
      "condition": {
        "type": "origins:resource",
        "resource": "mars:flight_flying_bar",
        "comparison": "<",
        "compare_to": 14
      }
    },
    "hidden": true
  },
  "bar_on_off_decrease": {
    "type": "origins:action_over_time",
    "interval": 30,
    "entity_action": {
      "type": "origins:execute_command",
      "command": "resource change @s mars:flight_flying_bar -1",
      "permission_level": 4
    },
    "condition": {
      "type": "origins:fall_flying",
      "inverted": false
    },
    "hidden": true
  },
  "bar_on_off_decrease_nosun": {
    "type": "origins:action_over_time",
    "interval": 30,
    "entity_action": {
      "type": "origins:execute_command",
      "command": "resource change @s mars:flight_flying_bar -1",
      "permission_level": 4
    },
    "condition": {
      "type": "origins:exposed_to_sun",
      "inverted": true
    },
    "hidden": true
  },
  "bar_recharge": {
    "type": "origins:action_over_time",
    "interval": 100,
    "entity_action": {
      "type": "origins:execute_command",
      "command": "resource change @s mars:flight_flying_bar 1",
      "permission_level": 4
    },
    "condition": {
      "type": "origins:and",
      "conditions": [
        {
          "type": "origins:exposed_to_sun"
        },
        {
          "type": "origins:fall_flying",
          "inverted": true
        }
      ]
    }
  },
  "name": "Flight",
  "description": "For a limited time only you can fly!"
}