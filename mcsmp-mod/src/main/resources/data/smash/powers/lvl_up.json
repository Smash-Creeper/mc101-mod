{
    "type": "origins:multiple",

    
    "level_up": {
        "type": "origins:action_over_time",
        "interval": 1,
        "rising_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
                    "command": "title @s actionbar {\"text\":\"1. Vanish / 2. Unvanish\",\"color\":\"#D6D6D6\",\"italic\":false}"
                }
            ]
        },
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:xp_levels",
                    "comparison": "<=",
                    "compare_to": 5
                }
            ]
        }
    }
}